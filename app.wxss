  text-align: center;
  width: 80rpx;
  height: 110rpx;
  vertical-align: middle;
}

.kuaidi-icon image {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.kuaidi-icon .line {
  height: 100%;
  position: relative;
}

tr:not(.finish) .kuaidi-icon .line:before {
  content: " ";
  display: block;
  width: 1rpx;
  background: #666;
  position: absolute;
  height: -webkit-calc(50% - 16rpx);
  top: 0;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}

tr:not(.start) .kuaidi-icon .line:after {
  content: " ";
  display: block;
  width: 1rpx;
  background: #666;
  position: absolute;
  height: -webkit-calc(50% - 16rpx);
  bottom: 0;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}

