/***样式修正***/

table {
  display: table;
  table-layout: fixed;
  width: 100%;
  box-sizing: border-box;
}

td {
  display: table-cell;
  vertical-align: middle;
  white-space: nowrap;
}

tr {
  display: table-row;
}

ul {
  display: block;
}

li {
  display: block;
}

div {
  display: block;
}

/***通用***/

.space {
  height: 20rpx;
}

.space-gray {
  height: 20rpx;
  background: #eeeef3;
  /* z-index: -1; */
  position: relative;
  margin-bottom: 20rpx;
}

.space-bg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  background: #eeeef3;
}

/* 地区选择 */

.areaselect {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}

.areaselect-btn {
  float: right;
  height: 60rpx;
  line-height: 60rpx;
  color: white;
  padding: 0 37rpx;
  border-radius: 30rpx;
  background: #ff6a00;
  font-size: 32rpx;
  clear: both;
}

.areaselect picker-view {
  width: 100%;
  height: 500rpx;
}

/* 空数据 */

.nodata {
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.nodata.static {
  position: initial;
  padding: 20rpx 0;
  text-align: center;
  -webkit-transform: none;
  transform: none;
}

.nodata image {
  width: 213rpx;
  height: 202rpx;
}

.nodata text {
  font-size: 30rpx;
  text-align: center;
  display: block;
  color: #999;
}

/* 过程 */

.process {
  display: inline-block;
  width: 100%;
  padding: 20rpx 0 10rpx 0;
}

.process li {
  float: left;
  width: 33.33%;
  text-align: center;
  position: relative;
}

.process-index {
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
  line-height: 44rpx;
  box-sizing: border-box;
  border: 4rpx solid #dbdbdb;
  border-radius: 26rpx;
  font-size: 28rpx;
  color: #ccc;
}

.process .active .process-index {
  border-color: #00c112;
  color: #00c112;
}

.process-text {
  font-size: 30rpx;
  color: #999;
  padding: 10rpx 0;
}

.process-time {
  font-size: 24rpx;
  color: #999;
}

.process-line {
  position: absolute;
  width: -webkit-calc(100% - 52rpx);
  height: 6rpx;
  background: #dbdbdb;
  left: -50%;
  -webkit-transform: translateX(26rpx);
  transform: translateX(26rpx);
  top: 26rpx;
}

.process .active .process-line {
  background: #00c112;
}

/* 图片列表 */

.editMessage_choose {
  display: inline-block;
  width: 100%;
}

.editMessage_choose li {
  float: left;
  width: -webkit-calc((100% - 60rpx) / 4);
  margin-bottom: 20rpx;
  margin-right: 20rpx;
}

.editMessage_choose li:nth-child(4n) {
  margin-right: 0;
}

.editMessage_choose li div {
  background: center center no-repeat;
  background-size: 100% auto;
  padding-bottom: 100%;
  width: 100%;
}

.editMessage_choose li div image {
  height: 50rpx;
  width: 50rpx;
}

/* 页面底部 */

.pagebottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 130rpx;
  background: #f9f9f9;
  box-sizing: border-box;
  border-top: 1rpx solid #b2b2b2;
}

/*字体颜色*/

.txt-yellow {
  color: #ff6a00;
}

.txt-green {
  color: #67cd35;
}

.txt-gray {
  color: #999;
}

/*下拉选择*/

.picker {
  font-size: 28rpx;
  color: #222;
}

.picker image {
  width: 20rpx;
  height: 11rpx;
  margin-left: 10rpx;
  position: relative;
  top: -4rpx;
}

/*单选*/

.radio {
  display: inline-block;
  color: #666;
  font-size: 24rpx;
}

.radio:before {
  content: " ";
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  border: 1rpx solid #737373;
  border-radius: 15rpx;
  box-sizing: border-box;
  position: relative;
  top: 4rpx;
  margin-right: 18rpx;
}

.radio image {
  display: none;
  width: 30rpx;
  height: 30rpx;
}

.radio.checked:before {
  display: none;
}

.radio.checked {
  color: #ff6a00;
}

.radio.checked image {
  display: inline-block;
  position: relative;
  top: 4rpx;
  margin-right: 18rpx;
}

/*表单*/

.form {
  table-layout: fixed;
  width: 100%;
  box-sizing: border-box;
  color: #222;
  font-size: 28rpx;
}

.form td {
  text-align: left;
  border-bottom: 1rpx solid #e0e0e0;
}

.form td:not(.label) {
  padding: 0 20rpx;
}

.form .label {
  width: 120rpx;
  padding: 0 20rpx;
  height: 90rpx;
}

.form .select image {
  width: 16rpx;
  height: 25rpx;
}

.form .select image {
  float: right;
  position: relative;
  top: 12rpx;
}

.form .select input {
  float: left;
  display: block;
  width: -webkit-calc(100% - 20rpx);
}

.form .textareatr .label {
  vertical-align: top;
  padding-top: 20rpx;
}

.form .textareatd {
  padding: 20rpx 0;
  height: 140rpx;
}

.form .textareatd textarea {
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
}

/*带横线标题*/

.title-mid {
  text-align: center;
  font-size: 28rpx;
  color: #666;
}

.title-mid:before {
  content: " ";
  display: inline-block;
  height: 4rpx;
  width: 30rpx;
  background: #d9d9d9;
  position: relative;
  top: -8rpx;
  margin-right: 12rpx;
}

.title-mid:after {
  content: " ";
  display: inline-block;
  height: 4rpx;
  width: 30rpx;
  background: #d9d9d9;
  position: relative;
  top: -8rpx;
  margin-left: 12rpx;
}

/*开关*/

.openclose {
  display: inline-block;
  width: 96rpx;
  height: 52rpx;
  line-height: 52rpx;
  border-radius: 26rpx;
  background: #aaa;
  font-size: 24rpx;
  color: white;
  text-align: center;
  position: relative;
}

.openclose text {
  width: 50%;
  float: left;
  display: block;
}

.openclose checkbox {
  display: none;
}

.openclose:before {
  content: " ";
  position: absolute;
  display: block;
  height: 44rpx;
  width: 44rpx;
  border-radius: 22rpx;
  left: 4rpx;
  top: 4rpx;
  background: white;
}

.openclose.checked:before {
  left: initial;
  right: 4rpx;
}

.openclose.checked {
  background: #13ad1d;
}

/*大按钮*/

.bigbutton {
  font-size: 36rpx;
  color: white;
  text-align: center;
  background: #ff6a00;
  width: 347rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 45rpx;
  margin: 40rpx auto;
}

